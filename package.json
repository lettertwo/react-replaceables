    "di",
    "ioc",
    "dependency injection"
  ],
  "author": "Eric Eldredge <lettertwo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lettertwo/react-replaceables/issues"
  },
  "homepage": "https://github.com/lettertwo/react-replaceables#readme",
  "main": "./umd/index.js",
  "module": "./esm/index.js",
  "lint-staged": {
