    "@babel/preset-flow": "7.0.0-beta.42",
    "@babel/preset-react": "7.0.0-beta.42",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "8.2.2",
    "babel-jest": "23.0.0-alpha.0",
    "eslint": "4.18.2",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-prettier": "2.6.0",
    "flow-bin": "0.68.0",
    "flow-typed": "2.3.0",
    "husky": "0.14.3",
    "jest": "22.4.2",
    "lint-staged": "7.0.0",
    "prettier": "1.11.1",
    "prettier-eslint": "8.8.1",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
