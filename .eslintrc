{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "prettier"
  ],
  "extends": [
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "prettier/prettier": [2, {"trailingComma": "es5", "singleQuote": true, "bracketSpacing": false, "parser": "babylon"}]
  }
}
